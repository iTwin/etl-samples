{
  "name": "etl-samples",
  "description": "Extract, Transform, and Load (ETL) samples that demonstrate export, transformation, and import of data from/to iModels",
  "license": "MIT",
  "engines": {
    "node": ">=10.16.0 <13.0"
  },
  "scripts": {
    "build": "tsc 1>&2 && npm run copy:test-assets",
    "clean": "rimraf lib",
    "copy:test-assets": "cpx \"./src/test/assets/**/*\" ./lib/test/assets",
    "lint": "eslint -f visualstudio \"./src/**/*.ts\" 1>&2",
    "test": "mocha lib/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs/tree/master/core/backend"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "ETL",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "devDependencies": {
    "@itwin/build-tools": "rc",
    "@itwin/eslint-plugin": "rc",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2.7",
    "chai": "^4.3.4",
    "cpx": "^1.5.0",
    "eslint": "^7.11.0",
    "mocha": "^5.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.1"
  },
  "dependencies": {
    "@bentley/itwin-client": "3.0.0-dev.148",
    "@itwin/browser-authorization": "latest",
    "@itwin/core-backend": "rc",
    "@itwin/core-bentley": "rc",
    "@itwin/core-common": "rc",
    "@itwin/core-geometry": "rc",
    "@itwin/core-i18n": "rc",
    "@itwin/core-transformer": "rc",
    "@itwin/ecschema-metadata": "rc"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": "plugin:@itwin/itwinjs-recommended"
  }
}
